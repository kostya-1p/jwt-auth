{
    "name": "kostya-p/jwt-auth",
    "description": "JSON Web Token Authentication",
    "type": "library",
    "require": {
        "php": "^8.2",
        "lcobucci/jwt": "^5.2",
        "nesbot/carbon": "^2.0",
        "illuminate/support": "^10.46",
        "illuminate/contracts": "^10.46",
        "illuminate/auth": "^10.46",
        "lcobucci/clock": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^1.10"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kostyap\\JwtAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kostyap\\JwtAuth\\test\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Kostyap\\JWTAuth\\Providers\\JwtAuthServiceProvider"
            ]
        }
    }
}
